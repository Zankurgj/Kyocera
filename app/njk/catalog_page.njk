{% extends "app/njk/layout/main.njk" %}
{% block content %}
  <div class="container">
    {% from "app/njk/macro.njk" import breadcrumbs %}
      {{ breadcrumbs([['Каталог']]) }}
  </div>
  <div class="page-catalog">
    <div class="container container--catalog">
      <div class="row row--wrap catalog-inner">

        <div class="catalog-sidebar-col">
          <aside class="catalog-sidebar">
            <p class="catalog-sidebar-head">ФИЛЬТРЫ</p>
            <div class="catalog-sidebar-block">
              <p class="catalog-sidebar-title">ТИП УСТРОЙСТВА</p>
              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input">
                <span class="checkbox"></span>
                <span class="checkbox-text">Принтер</span>
              </label>

              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input" checked>
                <span class="checkbox"></span>
                <span class="checkbox-text">МФУ</span>
              </label>
            </div>

            <div class="catalog-sidebar-block">
              <p class="catalog-sidebar-title">ЦВЕТНОСТЬ ПЕЧАТИ</p>
              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input">
                <span class="checkbox"></span>
                <span class="checkbox-text">Чёрно - белая</span>
              </label>

              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input" checked>
                <span class="checkbox"></span>
                <span class="checkbox-text">Цвет</span>
              </label>
            </div>

            <div class="catalog-sidebar-block">
              <p class="catalog-sidebar-title">ФОРМАТ</p>
              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input">
                <span class="checkbox"></span>
                <span class="checkbox-text">A4</span>
              </label>

              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input" checked>
                <span class="checkbox"></span>
                <span class="checkbox-text">A3</span>
              </label>
            </div>

            <div class="catalog-sidebar-block">
              <p class="catalog-sidebar-title">СКОРОСТЬ ПЕЧАТИ</p>
              <div class="catalog-range-wrapper">
                <div class="catalog-range-inputs">
                  <label class="catalog-range-input-wrapper">
                    от
                    <input class="catalog-range-input" type="text" id="firstVal" readonly>
                  </label>
                  <label class="catalog-range-input-wrapper">
                    до
                    <input class="catalog-range-input" type="text" id="nextVal" readonly>
                  </label>
                </div>
                <div class="catalog-range"></div>
              </div>
            </div>

            <div class="catalog-sidebar-block">
              <p class="catalog-sidebar-title">ДВУСТОРОННЯЯ ПЕЧАТЬ</p>
              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input">
                <span class="checkbox"></span>
                <span class="checkbox-text">Да</span>
              </label>

              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input" checked>
                <span class="checkbox"></span>
                <span class="checkbox-text">Нет</span>
              </label>

              <label class="checkbox-wrapper catalog-sidebar-checkbox">
                <input type="checkbox" class="checkbox-input">
                <span class="checkbox"></span>
                <span class="checkbox-text">Неважно</span>
              </label>
            </div>

          </aside>
        </div>

        <div class="catalog-items-col">
          <div class="row row--wrap">
              {% for i in range(0, 16) %}
                <div class="catalog-item-col">
                  {% from "app/njk/macro.njk" import productCard %}
                  {{ productCard(false, false, "./images/dest/img/product_card1.png") }}
                </div>
              {% endfor %}
          </div>
        </div>
      </div>
    </div>
